{
  "name": "@groovy-knowledge-search/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite-node --watch src/index.ts",
    "build": "vite build",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "vite": "^5.0.12",
    "vite-node": "^1.2.1",
    "vite-plugin-node": "^3.1.0",
    "vitest": "^1.2.1",
    "@vitest/coverage-v8": "^1.2.1"
  }
}
